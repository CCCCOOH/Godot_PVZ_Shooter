$(function(){if(!isMobileSite&&$("#rescViewHeadCat").height()>$("#rescViewHeadMain").height()){$("#rescViewHeadMain").height($("#rescViewHeadCat").height())}});gei.core.eventBus.on("Event_pageContentLoadEnd",function(){initResView()});var ItemContentTabDetail="Detail";var ItemContentTabGroup="Group";var ItemContentTabHot="hot";var currentItemContentTab;if($("#rcDomainIdVal").val()!==""){currentItemContentTab=ItemContentTabHot}else{currentItemContentTab=ItemContentTabDetail}var cacheRecommendData="";function initResView(){if($(".resc-view-group").size()>0){$("body").structNav({navGroupEls:$(".resc-view-group"),rootNavName:$(".page-title-big a").html()})}$(".struct-nav-fixed-container").hide();$('.res-detail-nav a[data-toggle="tab"]').unbind("click").bind("click",function(f){if($(f.currentTarget).attr("href")=="#resDirList"){$(".struct-nav-fixed-container").show();$(".struct-nav-fixed").hide();resContentLoad(ItemContentTabGroup)}else{if($(f.currentTarget).attr("href")=="#resHot"){$(".struct-nav-fixed-container").show();$(".struct-nav-fixed").hide();resContentLoad(ItemContentTabHot)}else{$(".struct-nav-fixed-container").hide();resContentLoad(ItemContentTabDetail)}}$(this).tab("show")});var b="rgroup_init";var a="ret_success";var d=getStrPos(b,1);var e=getStrPos(a,2);var c=d+e+getStrPos(a,5)+"c-d";$("#"+c).unbind("click").click(function(){zipGet(this)})}function loadNewRecommendData(){gei.core.eventBus.fire("Event_pageContentLoadStart");NProgress.start();var a=$("#rcDomainIdVal").val();$("#res-tab-content").html('<div id="tmpLoadData" style="text-align: center;" ><i class="icon-spinner icon-spin icon-4x" style="color: #0f98ff"/></div>');$.get("/gei-list/ajaxComp/feed/rc/domain/feed",{domainId:a,pageNum:$("#feedPageNum").val(),pageSize:$("#feedPageSize").val()},function(b){$("#res-tab-content").html(b);gei.core.eventBus.fire("Event_pageContentLoadEnd");NProgress.done()})}function loadCachedRecommendData(){gei.core.eventBus.fire("Event_pageContentLoadStart");$("#res-tab-content").html(cacheRecommendData);gei.core.eventBus.fire("Event_pageContentLoadEnd")}function resContentLoad(a){if(a==="hot"){if(currentItemContentTab==="hot"){loadNewRecommendData()}else{if(cacheRecommendData!==""){loadCachedRecommendData()}else{loadNewRecommendData()}}}else{if(currentItemContentTab==="hot"){cacheRecommendData=$("#res-tab-content").html()}$.get(groupContentAjaxUrlPrex+currentGroupQueryUrl+".html",{template:a},function(b){gei.core.eventBus.fire("Event_pageContentLoadStart");$("#res-tab-content").empty(b);$("#res-tab-content").removeClass("res-tab-content-"+ItemContentTabDetail);$("#res-tab-content").removeClass("res-tab-content-"+ItemContentTabGroup);$("#res-tab-content").addClass("res-tab-content-"+a);$("#res-tab-content").html(b);gei.core.eventBus.fire("Event_pageContentLoadEnd");NProgress.done()})}currentItemContentTab=a}var viewPageTarget=null;var ajaxPageUrlPool={};function viewPage(g,h,j,i){var e=$(i);var a=g==""?0:g-1;var c=e.parents(".resc-view-group");c.data("itemOrder",j);var d="itemInfoPageBox_"+h+"_GroupContent_"+a+"_"+j;var f=$("#"+d);gei.core.eventBus.fire("Event_pageContentLoadStart");if(f.size()==0){gei.util.tipLoading.start(e);$.get(groupContentAjaxUrlPrex+h+".html",{page:a,order:j,template:"GroupContent"},function(m){var l=$("[itembox]",$('div[class="item-info-list-box"]',c));f=$(m).insertAfter($(".item-info-list-box",c).last());f.data("html",m);gei.util.tipLoading.done(e);showPageBox(f,l,c);gei.core.eventBus.fire("Event_pageContentLoadEnd");gei.core.eventBus.fire("Event_groupContentLoadEnd",{pageBoxId:d,container:c,pageBox:f})})}else{var b=$("[itembox]",$('div[class="item-info-list-box"]',c));var k=f.data("html");f.detach();f=$(k);f.data("html",k);f.insertAfter($(".item-info-list-box",c).last());showPageBox(f,b,c);gei.core.eventBus.fire("Event_pageContentLoadEnd");gei.core.eventBus.fire("Event_groupContentLoadEnd",{pageBoxId:d,container:c,pageBox:f})}if(e&&e.attr("css-active")){$(".link",c).removeClass("active");e.addClass("active")}}function showPageBox(d,a,c){var b=d.attr("id");$("[itembox]",d).each(function(e,g){var f=null;$.each(a,function(h,i){if($(i).attr("itembox")==$(g).attr("itembox")){f=$(i);return false}});if(f){f.insertAfter($(g));$(g).remove()}});$(".item-info-list-box",c).each(function(e,f){if($(f).attr("id")!=b){$(f).hide()}else{$(f).show()}})}function slideToGroup(a){slideTo($("#group_"+a),(navFixed.height()+7)*-1)}function getStrPos(b,c){b=b+"";var a=b.substring(c-1,c);return a}function zipGet(b){var a=$(b);a.button("loading");setTimeout(function(){a.button("reset")},3000);fileGet(b,"down")};